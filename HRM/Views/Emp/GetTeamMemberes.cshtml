@model IEnumerable<DataAccessLayer.AspNetUser>

@{
    ViewBag.Title = "GetTeamMembers";
    if (ViewBag.deptid == "D4")
    {
        Layout = "~/Views/Shared/_LayoutPageAdmin.cshtml";
    }
    else
    {
        Layout = "~/Views/Shared/_LayoutPageEmp.cshtml";
    }
    //Layout = "~/Views/Shared/_LayoutPageEmp.cshtml";
}

<h2 class="text-center">My Team Info</h2>

<div class="container" style="margin-top:10px">

<div class="row">

    <div class="col-lg-12 col-lg-offset-0 alert alert-dark">
        <table class="table">
            <tr>
                <th>
                    @Html.DisplayNameFor(model => model.Email)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.PhoneNumber)
                </th>

                <th>
                    @Html.DisplayNameFor(model => model.UserName)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.Dept.DepartmentName)
                </th>
                <th></th>
            </tr>

            @foreach (var item in Model)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.Email)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.PhoneNumber)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.UserName)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Dept.DepartmentName)
                </td>
            </tr>
    }

        </table>
        <button class="col-md-2 col-md-offset-5 btn btn-dark " onclick="location.href='@Url.Action("EmptyLayout", "Emp")'"> Back</button>
    </div>
    </div>
    </div>
